package actions;

import vo.News;

import com.opensymphony.xwork2.ActionSupport;

@SuppressWarnings("serial")
public class NewsAction extends ActionSupport
{
	private News news = new News();
	public News getNews()
	{
		return news;
	}
	
	public String insert()
	{
		System.out.println("******增加新闻操作******");
		return null;
	}
	
	public void update()
	{
		System.out.println("******修改新闻操作******");
	}
	
	public void list()
	{
		System.out.println("*****列出新闻操作******");
	}
}
