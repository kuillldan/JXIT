package org.lyk.main;

import java.lang.reflect.Constructor;
import java.lang.reflect.Field;
import java.lang.reflect.Method;
import java.lang.reflect.Modifier;
import java.sql.*;
import java.text.*;
import java.util.*;

import utils.StringUtils;
 
public class Main  
{
	public static void main(String[] args) throws Exception
	{   
		Class cls = Class.forName("org.lyk.vo.Dept");
		Object obj = cls.newInstance();
		
		Field dname = cls.getDeclaredField("dname");
		Field loc = cls.getDeclaredField("loc");
		Field deptno = cls.getDeclaredField("deptno");
		
		System.out.println("Type of dname:" + dname.getType().getSimpleName());
		System.out.println("Type of loc:" + loc.getType().getSimpleName());
		
		System.out.println("///Main done~~")  ;
	}
}
    